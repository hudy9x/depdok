# Run `pnpm tauri signer generate` to generate private and public key
TAURI_SIGNING_PRIVATE_KEY=

# Polar.sh organization ID for license validation
VITE_POLAR_ORG_ID=your_polar_org_id_here

# Tauri Backend License Configuration
TAURI_POLAR_API_URL=https://api.polar.sh/v1/customer-portal/license-keys/validate
# Development bypass key (only works in debug builds). Set to enable quick testing without Polar.sh API.
# Leave empty or comment out to test real Polar.sh license validation in development mode.
TAURI_DEV_LICENSE_KEY=test-key-12345
# Grace period in days (default: 30, set to 0 to test expired state)
TAURI_GRACE_PERIOD_DAYS=30